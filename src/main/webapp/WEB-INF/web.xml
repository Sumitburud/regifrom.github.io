<?xml version="1.0" encoding="UTF-8"?>
<web-app xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xmlns="http://xmlns.jcp.org/xml/ns/javaee" xsi:schemaLocation="http://xmlns.jcp.org/xml/ns/javaee http://xmlns.jcp.org/xml/ns/javaee/web-app_4_0.xsd" id="WebApp_ID" version="4.0">
  <display-name>Hospital_Management</display-name>
  <welcome-file-list>
    <welcome-file>index.html</welcome-file>
    <welcome-file>index.jsp</welcome-file>
    <welcome-file>index.htm</welcome-file>
    <welcome-file>default.html</welcome-file>
    <welcome-file>default.jsp</welcome-file>
    <welcome-file>default.htm</welcome-file>
  </welcome-file-list>
  
  <servlet>
   <servlet-name>fun1</servlet-name>
   <servlet-class>com.HospitalCarePlus.Function1</servlet-class>
  
  </servlet>
  <servlet-mapping>
  
  <servlet-name>fun1</servlet-name>
  <url-pattern>/callFunction1</url-pattern>
  </servlet-mapping>
  
  
  <servlet>
  <servlet-name>doc</servlet-name>
  <servlet-class>com.HospitalCarePlus.DoctorLogin</servlet-class>
  </servlet>
  
  <servlet-mapping>
  <servlet-name>doc</servlet-name>
  <url-pattern>/callDoctor</url-pattern>
  </servlet-mapping>
  
  
  <servlet>
  
  <servlet-name>Details</servlet-name>
  <servlet-class>com.HospitalCarePlus.PatientDetails</servlet-class>
  </servlet>
  
  <servlet-mapping>
  <servlet-name>Details</servlet-name>
  <url-pattern>/CallFunction2</url-pattern>
  </servlet-mapping>
  
  <servlet>
  <servlet-name>Profile</servlet-name>
  <servlet-class>com.HospitalCarePlus.PatientLoginAccess</servlet-class>
  </servlet>
  
  <servlet-mapping>
  <servlet-name>Profile</servlet-name>
  <url-pattern>/PatientLogin</url-pattern>
  </servlet-mapping>
  
  
  <servlet>
  <servlet-name>Profile2</servlet-name>
  <servlet-class>com.HospitalCarePlus.Appointment</servlet-class>
  </servlet>
  
  <servlet-mapping>
  <servlet-name>Profile2</servlet-name>
  <url-pattern>/CallAppointment</url-pattern>
  </servlet-mapping>
  
  
  
  
   
</web-app>